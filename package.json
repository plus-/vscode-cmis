{
	"name": "vscode-cmis",
	"author": {
		"name": "Patrick Lussan",
		"email": "lussanp@gmail.com"
	},
	"displayName": "CMIS FileSystem",
	"description": "Enable access to ECMs supporting CMIS",
	"version": "0.0.1",
	"repository": {
		"url": "https://github.com/plus-/vscode-cmis.git",
		"type": "git"
	},
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onFileSystem:cmis",
		"onCommand:cmis.workspaceInit",
		"onCommand:cmis.init",
		"onCommand:cmis.reset"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [
			{
				"command": "cmis.workspaceInit",
				"title": "Setup Workspace",
				"category": "CMIS FileSystem"
			},
			{
				"command": "cmis.init",
				"title": "Create Files",
				"category": "CMIS FileSystem"
			},
			{
				"command": "cmis.reset",
				"title": "Delete Files",
				"category": "CMIS FileSystem"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "cmis.init",
					"when": "workbenchState == workspace"
				},
				{
					"command": "cmis.reset",
					"when": "workbenchState == workspace"
				},
				{
					"command": "cmis.workspaceInit",
					"when": "workbenchState != workspace"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"dependencies": {
		"cmis": "^1.0.1"
	},
	"devDependencies": {
		"@types/node": "^7.0.43",
		"tslint": "^5.11.0",
		"typescript": "^2.5.2",
		"vscode": "^1.1.17"
	}
}